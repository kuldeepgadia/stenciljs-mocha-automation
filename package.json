{
  "name": "stencil-starter-project-name",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test:e2e": "stencil test --e2e --spec",
    "test": "mocha --timeout 90000  ./test/bootstrap.js --recursive test --reporter mochawesome",
    "test-feature": "mocha --timeout 90000  ./test-feature/bootstrap.js --recursive test-feature --reporter mochawesome",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.0",
    "@stencil/router": "^1.0.1",
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^5.4.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1",
    "node-cmd": "^3.0.0",
    "opn": "^6.0.0",
    "puppeteer": "^5.3.1",
    "query-selector-shadow-dom": "^0.7.1"
  },
  "license": "MIT"
}
